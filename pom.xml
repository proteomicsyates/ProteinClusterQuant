			<groupId>edu.scripps.yates</groupId>
			<artifactId>proteoform_dbindex</artifactId>
			<version>0.1.3-SNAPSHOT</version>
			<exclusions>
				<exclusion>
					<groupId>ch.qos.logback</groupId>
