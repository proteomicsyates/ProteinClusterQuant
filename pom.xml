			</exclusions>
		</dependency>
		<dependency>
			<groupId>edu.scripps.yates</groupId>
			<artifactId>utilities</artifactId>
			<version>1.6-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>org.slf4j</groupId>
			<artifactId>slf4j-simple</artifactId>
			<version>1.7.25</version>
		</dependency>
		<!-- To enable machine optimized matrix computation, the users should add the dependency of smile-netlib: -->
		<dependency>
	      <groupId>com.github.haifengl</groupId>
	      <artifactId>smile-netlib</artifactId>
	      <version>1.5.2</version>
	    </dependency>
		 
	</dependencies>
	<profiles>
	
	
      <profile>
         <id>PCQ</id>
<!--          <activation> -->
<!--             <activeByDefault>true</activeByDefault> -->
<!--          </activation> -->
         <build>
            <plugins>
            	<plugin>
				<artifactId>maven-jar-plugin</artifactId>
				<version>2.3.2</version>
				<configuration>
					<archive>
						<manifest>
							<addClasspath>true</addClasspath>
							<mainClass>edu.scripps.yates.pcq.ProteinClusterQuant</mainClass>
							<classpathPrefix>lib</classpathPrefix>
							<useUniqueVersions>false</useUniqueVersions>
						</manifest>
					</archive>
					<outputDirectory>${project.build.directory}</outputDirectory>
					<finalName>PCQ_${project.version}</finalName>
				</configuration>
				</plugin>
               <plugin>
                  <artifactId>maven-assembly-plugin</artifactId>
                  <version>3.1.0</version>
                  <executions>
                     <execution>
                        <id>assembly</id>
                        <phase>package</phase>
                        <goals>
                           <goal>single</goal>
                        </goals>
                     </execution>
                  </executions>
                  <configuration>
                     <finalName>${assembly.dir}</finalName>
                     <descriptors>
                        <descriptor>${basedir}/src/main/resources/assembly.xml</descriptor>
                     </descriptors>
                     <appendAssemblyId>false</appendAssemblyId>
                     <archive>
		               	  <manifest>
		                     <addClasspath>true</addClasspath>
		                     <mainClass>edu.scripps.yates.pcq.ProteinClusterQuant</mainClass>
		                     <classpathPrefix>lib/</classpathPrefix>
		                     <useUniqueVersions>false</useUniqueVersions>
		                  </manifest>
		               </archive>
                  </configuration>
               </plugin>
            </plugins>
         </build>
      </profile>
